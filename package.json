{
  "name": "seed-api-server",
  "version": "0.2.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint 'lib/**/*.js'",
    "lint:msg": "validate-commit-msg",
    "start": "node .",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "mkdir": "mkdirp dist",
    "build": "npm-run-all clean mkdir -p babel",
    "build:dev": "npm-run-all clean mkdir babel:dev",
    "babel": "babel src -d dist --ignore spec.js,test.js",
    "babel:dev": "babel src -s -D -d dist",
    "deploy": "npm run build && npm run docker:build",
    "flow": "flow",
    "flow:coverage": "flow-coverage-report",
    "commit": "git cz",
    "precommit": "npm run lint",
    "commitmsg": "npm run lint:msg",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1",
    "docker:images": "docker images | grep seed-api-server",
    "docker:rm": "docker rm -f seed-api-server",
    "docker:run": "docker run -dit -p 8082:8082 --name seed-api-server seed-api-server:latest",
    "docker:build": "docker build --tag seed-api-server . && npm run docker:images"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhiokim/seed-api-server.git"
  },
  "keywords": [],
  "author": "Rhio Kim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhiokim/seed-api-server/issues"
  },
  "homepage": "https://github.com/rhiokim/seed-api-server#readme",
  "dependencies": {
    "express": "^4.16.2",
    "express-routemap": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.5",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.12.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.59.0",
    "flow-coverage-report": "^0.4.0",
    "flow-typed": "^2.2.3",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.1.0",
    "rimraf": "^2.6.2",
    "validate-commit-msg": "^2.14.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-flow-strip-types"
    ]
  },
  "flow-coverage-report": {
    "includeGlob": [
      "src/**/*.js"
    ],
    "excludeGlob": [
      "src/**/__tests__/**/*.js",
      "src/**/?(*.){test,spec}.js"
    ],
    "threshold": 10,
    "type": [
      "text"
    ]
  },
  "config": {
    "validate-commit-msg": {
      "preset": "angular",
      "warnOnFail": false,
      "maxSubjectLength": 100
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
